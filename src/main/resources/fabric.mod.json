{
	"schemaVersion": 1,
	"id": "rewind",
	"version": "${version}",
	"name": "Rewind - Time Machine",
	"description": "A time machine mod that records world changes and allows rewinding the world state up to 30 seconds. Use /timeline rewind <seconds> to restore the past!",
	"authors": [
		"Rewind Team"
	],
	"contact": {
		"homepage": "https://github.com/rewind-mod/rewind",
		"sources": "https://github.com/rewind-mod/rewind"
	},
	"license": "MIT",
	"icon": "assets/rewind/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"io.github.rewind.RewindMod"
		]
	},
	"mixins": [
		"rewind.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": "~1.21.11",
		"java": ">=21",
		"fabric-api": "*"
	}
}